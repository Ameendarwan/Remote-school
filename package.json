{
  "name": "takmil-backend",
  "version": "0.0.2",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"apps/**/*.ts\" \"libs/**/*.ts\"",
    "start": "nest start",
    "start:dev:azure:school": "nest build school && cd ./apps/school && func host start --verbose --typescript",
    "start:dev": "nest start --watch",
    "start:docker:dev": "docker-compose -f docker-compose.dev.yml up",
    "start:docker:prod": "docker-compose -f docker-compose.stag.yml up",
    "start:stag": "docker-compose -f docker-compose.prod.yml up",
    "start:auth:dev": "nest start auth --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/apps/sleepr/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "auth:test": "cross-env APP_ENV=testing jest --config apps/auth/test/unit-test/jest.json --passWithNoTests --watch",
    "school:test": "cross-env APP_ENV=testing jest --config apps/school/test/unit-test/jest.json --passWithNoTests --watch",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./apps/sleepr/test/jest-e2e.json",
    "release": "standard-version",
    "seed:school": "npm run rollback:org && npm run rollback:address && npm run seed:organization && npm run seed:address && nestjs-command seed:school",
    "seed:real:school": "npm run rollback:real:school && cross-env CLI_PATH=./apps/school/src/cli.ts nestjs-command seed:real:school",
    "rollback:real:school": "cross-env CLI_PATH=./apps/school/src/cli.ts nestjs-command remove:real:school",
    "seed:address": "cross-env DATABASE_HOST=mongodb://192.168.0.104:27017 CLI_PATH=./apps/auth/src/cli.ts nestjs-command seed:address",
    "seed:user": "npm run rollback:address && npm run rollback:user && npm run seed:address && cross-env DATABASE_HOST=mongodb://192.168.0.104:27017 CLI_PATH=./apps/auth/src/cli.ts nestjs-command seed:user",
    "seed:organization": "nestjs-command seed:organization",
    "rollback:organization": "nestjs-command remove:organization",
    "rollback:address": "cross-env DATABASE_HOST=mongodb://192.168.0.104:27017 CLI_PATH=./apps/auth/src/cli.ts nestjs-command remove:address",
    "rollback:school": "nestjs-command remove:school",
    "rollback:user": "cross-env DATABASE_HOST=mongodb://192.168.0.104:27017 CLI_PATH=./apps/auth/src/cli.ts nestjs-command remove:user",
    "rollback:teacher": "nestjs-command remove:teacher",
    "seed:teacher": "npm run rollback:address && npm run rollback:user && npm run seed:address && npm run seed:user && nestjs-command seed:teacher",
    "start:azure": "npm run build && func host start"
  },
